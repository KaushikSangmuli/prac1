package extra_Assignment;

public class Node {
private int val;
private Node next;
public int getVal() {
	return val;
}
public void setVal(int val) {
	this.val = val;
}
public Node getNext() {
	return next;
}
public void setNext(Node next) {
	this.next = next;
}
public Node(int v) {
	val=v;
    next=null;
}

public Node() {
    val= 0;
    next= null;
}


}
